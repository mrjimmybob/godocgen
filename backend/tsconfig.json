{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "lib": ["ES2020", "DOM"],
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "types": ["node", "express"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  